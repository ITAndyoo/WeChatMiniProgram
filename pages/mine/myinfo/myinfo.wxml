<view class="page">
<view wx:if="{{topTips}}" class="weui-toptips weui-toptips_warn fadeIn {{hide ? 'fadeOut' : ''}}">该功能正在开发~</view>
<!-- <view style="width:100%;height:25px;font-size:15px;position:fixed;top:0;text-align:center;background-color:tomato;" id="topTips" hidden="{{tips_hid}}">该功能正在开发~</view> -->
<view style="text-align:center">
  <image wx:if="{{!flag}}" src ="../../../images/logo.png" style="min-width:240px;width:240px;min-height:180px;height:180px"/>
  <image wx:if="{{flag}}" src ="{{mlurl}}static/{{logo}}" style="min-width:240px;width:98%;"/>
</view>
<view class="page card" data-weui-theme="{{theme}}">
<view class="weui-panel weui-panel_access ">
  <view class="weui-panel__bd">
      <view bindtap="settings" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
          <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
              <image wx:if = "{{nickName == undefined || nickName == null || nickName == ''}}" class="weui-media-box__thumb" src="/images/avatar.png" />
              <image class="weui-media-box__thumb avatar" src="{{avatarUrl}}" />
          </view>
          <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
              <view class="weui-media-box__title"><text wx:if="{{is_admin == true}}" style="color:red;">{{nickName}}</text><text wx:if="{{nickName == undefined || nickName == null || nickName == ''}}">请登录</text></view>
              <view class="weui-media-box__title"><text wx:if="{{is_admin == false}}">{{nickName}}</text></view>
              <view class="weui-media-box__desc" wx:if="{{is_admin == true}}" style="color:red;">今天又是愉快的一天呢，加油！</view>
              <view class="weui-media-box__desc" wx:if="{{is_admin == false}}">每天都要好好的！</view>
          </view>
      </view>
  </view>
</view>
<view class="weui-panel">
  <view class="weui-panel__hd">我的</view>
  <view class="weui-panel__bd">
      <view class="weui-media-box weui-media-box_small-appmsg">
          <view class="weui-cells weui-cells_in-small-appmsg">
              <view class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active" bindtap="mark">
                  <view class="weui-cell__hd"><image src="data:image/svg+xml;base64," style="width: 20px;height: 20px;margin-right: 16px;vertical-align:middle;" /></view>
                  <view class="weui-cell__bd weui-cell_primary">
                      <view>我的收藏</view>
                  </view>
                  <view class="weui-cell__ft weui-cell__ft_in-access"></view>
              </view>
              <view wx:if="{{is_admin == true}}" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active" bindtap="record">
                  <view class="weui-cell__hd"><image src="data:image/svg+xml;base64," style="width: 20px;height: 20px;margin-right: 16px;vertical-align:middle;" /></view>
                  <view class="weui-cell__bd weui-cell_primary">
                      <view>文字管理<text style="color:red;">（管理员功能）</text></view>
                  </view>
                  <view class="weui-cell__ft weui-cell__ft_in-access"></view>
              </view>
              <view wx:if="{{is_admin == true}}" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active" bindtap="msg">
                  <view class="weui-cell__hd"><image src="data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHdpZHRoPSIyNCIgaGVpZ2h0PSIyNCIgdmlld0JveD0iMCAwIDI0IDI0Ij48cGF0aCBkPSJNMCAzdjE4aDI0di0xOGgtMjR6bTIxLjUxOCAybC05LjUxOCA3LjcxMy05LjUxOC03LjcxM2gxOS4wMzZ6bS0xOS41MTggMTR2LTExLjgxN2wxMCA4LjEwNCAxMC04LjEwNHYxMS44MTdoLTIweiIvPjwvc3ZnPg==" style="width: 20px;height: 20px;margin-right: 16px;vertical-align:middle;" /></view>
                  <view class="weui-cell__bd weui-cell_primary">
                      <!-- <view>我的消息</view> -->
                       <span style="vertical-align: middle;">我的消息<text style="color:red;">（管理员功能）</text></span>
                      <span class="weui-badge" style="margin-left: 5px;display:{{bd_hid}}">New</span>
                  </view>
                  <view class="weui-cell__ft weui-cell__ft_in-access"></view>
              </view>
          </view>
      </view>
  </view>
  <view class="weui-panel__hd">更多</view>
  <view class="weui-panel__bd">
      <view class="weui-media-box weui-media-box_small-appmsg">
          <view class="weui-cells weui-cells_in-small-appmsg">
              <navigator url="../secret/xunta" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
                  <view class="weui-cell__hd"><image src="data:image/svg+xml;base64," style="width: 20px;height: 20px;margin-right: 16px;vertical-align:middle;" /></view>
                  <view class="weui-cell__bd weui-cell_primary">
                      <view>关于XunTa</view>
                  </view>
                  <view class="weui-cell__ft weui-cell__ft_in-access"></view>
              </navigator>
              <button open-type="feedback" style="font-weight:400;text-align:left;width:100%;" url="" class="weui-cell weui-cell_example weui-cell_access feedbackbtn" hover-class="weui-cell_active">
                  <view class="weui-cell__hd"><image src="data:image/svg+xml;base64," style="width: 20px;height: 20px;margin-right: 16px;vertical-align:middle;"/></view>
                  <view class="weui-cell__bd weui-cell_primary">
                      <view>反馈建议</view>
                  </view>
                  <view class="weui-cell__ft weui-cell__ft_in-access"></view>
              </button>
              <view url="" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active" bindtap="quit">
                  <view class="weui-cell__hd"><image src="data:image/svg+xml;base64,PHN2ZyB4bWxucz0iaHR0cDovL3d3dy53My5vcmcvMjAwMC9zdmciIHdpZHRoPSIyNCIgaGVpZ2h0PSIyNCIgdmlld0JveD0iMCAwIDI0IDI0Ij48cGF0aCBkPSJNMTAgMnYyaDEydjE2aC0xMnYyaDE0di0yMGgtMTR6bTAgNy40MDhsMi45NjMgMi41OTItMi45NjMgMi41OTJ2LTEuNTkyaC04di0yaDh2LTEuNTkyem0tMi00LjQwOHY0aC04djZoOHY0bDgtNy04LTd6Ii8+PC9zdmc+" style="width: 20px;height: 20px;margin-right: 16px;vertical-align:middle;" /></view>
                  <view class="weui-cell__bd weui-cell_primary">
                      <view>退出登录</view>
                  </view>
                  <view class="weui-cell__ft weui-cell__ft_in-access"></view>
              </view>
          </view>
      </view>
  </view>
</view>
<view class="tabholder"></view>
</view>


<view class="fadeIn" wx:if="{{iosDialog1}}">
    <view class="weui-mask"></view>
    <view class="weui-dialog">
        <view class="weui-dialog__hd"><strong class="weui-dialog__title">提示信息</strong></view>
        <view class="weui-dialog__bd">您尚未登录，是否登陆</view>
        <view class="weui-dialog__ft">
            <a class="weui-dialog__btn weui-dialog__btn_default" bindtap="close">取消</a>
            <a class="weui-dialog__btn weui-dialog__btn_primary" bindtap="confirm">确定登录</a>
        </view>
    </view>
</view>

<view class="fadeIn" wx:if="{{iosDialog2}}">
    <view class="weui-mask"></view>
    <view class="weui-dialog">
        <view class="weui-dialog__hd"><strong class="weui-dialog__title">提示信息</strong></view>
        <view class="weui-dialog__bd">你的账号已被封禁，如有疑问请联系管理员</view>
        <view class="weui-dialog__ft">
            <a class="weui-dialog__btn weui-dialog__btn_default" bindtap="close2">取消</a>
            <a class="weui-dialog__btn weui-dialog__btn_primary" bindtap="confirm2">知道了</a>
        </view>
    </view>
</view>


</view>
